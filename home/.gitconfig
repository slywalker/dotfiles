[core]
	excludesfile = /Users/slywalker/.gitignore
	pager = less -r
	editor = vim -c \"set fenc=utf-8\"
[color]
	ui = auto
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[merge]
	log = true
	tool = opendiff
[push]
	default = simple
[commit]
	cleanup = whitespace
[include]
	path = ~/.gitconfig.user
